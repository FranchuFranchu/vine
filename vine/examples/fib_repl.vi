
use std::{io::{prompt, println, read_byte, print_char}, u32, option::Option::{Some, None, is_some, unwrap}};

fn main(io) {
  let line;
  while { line = io.prompt("> "); line.is_some() } {
    let line = line.unwrap();
    let num = u32::parse(line);
    io.println(match num {
      Some(num) => u32::to_string(fib(num)),
      None => "invalid number",
    });
  }
}

fn fib(n) {
  let a = 0;
  let b = 1;
  while n {
    n -= 1;
    (a, b) = (b, a + b);
  }
  a
}
